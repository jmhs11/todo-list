{
  "name": "todo-list",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run env && ng build",
    "env": "ts-node ./.cicd/set-env.ts --environment=prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false --browsers ChromeHeadless",
    "lint": "npx eslint \"src/**/*.{js,jsx,ts,tsx,html}\" --quiet --fix",
    "format": "npx prettier \"src/**/*.{js,jsx,ts,tsx,html,css,scss}\" --write",
    "db": "json-server --watch ./src/db/db.json",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npx eslint . --fix"
    ]
  },
  "dependencies": {
    "@angular/animations": "13.3.8",
    "@angular/common": "13.3.8",
    "@angular/compiler": "13.3.8",
    "@angular/core": "13.3.8",
    "@angular/fire": "7.3.0",
    "@angular/forms": "13.3.8",
    "@angular/platform-browser": "13.3.8",
    "@angular/platform-browser-dynamic": "13.3.8",
    "@angular/router": "13.3.8",
    "angular-svg-icon": "13.0.0",
    "angular-svg-icon-preloader": "3.0.0",
    "firebase": "9.8.1",
    "rxjs": "7.5.5",
    "tslib": "2.4.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.10",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "13.3.5",
    "@angular/compiler-cli": "13.3.8",
    "@tailwindcss/forms": "0.5.2",
    "@tailwindcss/line-clamp": "0.4.0",
    "@types/jasmine": "4.0.3",
    "@types/node": "17.0.34",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "autoprefixer": "10.4.7",
    "dotenv": "16.0.1",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "8.0.1",
    "jasmine-core": "4.1.1",
    "json-server": "0.17.0",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "postcss": "8.4.14",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "0.1.11",
    "tailwindcss": "3.0.24",
    "ts-node": "10.7.0",
    "typescript": "4.6.4",
    "vercel": "24.2.3",
    "yargs": "17.5.1"
  }
}
